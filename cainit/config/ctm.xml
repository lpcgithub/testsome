<?xml version="1.0" encoding="utf-8"?>
<ctml>
	<basedescription>
		<name>子CA证书模板</name>
		<validitypolicy>
			<defaultValidity>365</defaultValidity>
			<maxValidity>365</maxValidity>
			<notBefore>2010-08-31</notBefore>
		</validitypolicy>
		<keypolicy>
			<keyType>rsa</keyType>
			<keyLength>1024</keyLength>
			<keyGenPlace>user</keyGenPlace>
		</keypolicy>
		<keyspecpolicy>
			<keyspec>sign</keyspec>
		</keyspecpolicy>
		<attribute>
			<type>single</type>
			<refCodeRequest>false</refCodeRequest>
		</attribute>
	</basedescription>
	<extensiondescription>
		<standardextension>
			<BasicConstraints>
				<name>BasicConstraints</name>
				<crucial>false</crucial>
				<userprocess>ALLOW</userprocess>
				<value>
					<ca>true</ca>
					<pathLenConstraint>2</pathLenConstraint>
				</value>
			</BasicConstraints>
			<CertificateTemplate />
			<CertificatePolicies />
			<CRLDistributionPoints />
			<ExtendKeyUsage />
			<KeyUsage />
			<ICRegistrationNumber />
			<IdentifyCode />
			<InsuranceNumber />
			<AuthorityKeyIdentifier />
			<AuthorityInformationAccess />
			<NetscapeCertType />
			<OrganizationCode />
			<PolicyMapping />
			<SubjectAltName />
			<SubjectKeyIdentifier />
			<TaxationNumber />
		</standardextension>
		<selfextension />
	</extensiondescription>
</ctml>


