<ctml>
	<basedescription>
		<name>系统服务证书模板</name>
		<validitypolicy>
			<defaultValidity>365</defaultValidity>
			<maxValidity>365</maxValidity>
			<notBefore>2010-12-31</notBefore>
		</validitypolicy>
		<keypolicy>
			<keyType>rsa</keyType>
			<keyLength>1024</keyLength>
			<keyGenPlace>user</keyGenPlace>
		</keypolicy>
		<keyspecpolicy>
			<keyspec>sign</keyspec>
		</keyspecpolicy>
		<attribute>
			<type>sys</type>
			<refCodeRequest>false</refCodeRequest>
		</attribute>
	</basedescription>
	<extensiondescription>
		<standardextension>
			<BasicConstraints />
			<CertificateTemplate />
			<CertificatePolicies />
			<CRLDistributionPoints />
			<ExtendKeyUsage />
			<KeyUsage />
			<ICRegistrationNumber />
			<IdentifyCode />
			<InsuranceNumber />
			<AuthorityKeyIdentifier />
			<AuthorityInformationAccess />
			<NetscapeCertType />
			<OrganizationCode />
			<PolicyMapping />
			<SubjectAltName />
			<SubjectKeyIdentifier />
			<TaxationNumber />
		</standardextension>
		<selfextension />
	</extensiondescription>
</ctml>